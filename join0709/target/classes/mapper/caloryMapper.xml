<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.join.mapper.caloryMapper">
<select id = "selectFoodName" resultType = "String" parameterType="java.util.HashMap">
	select 
		foodname
	from
		food
</select>

<insert id="insertCalory" parameterType="java.util.HashMap">
        insert into calory(
        	userid,
            breakfast,
            lunch,
            dinner,
            day
        )values(
        	#{userid},
            #{breakfast},
            #{lunch},
            #{dinner},
            #{day}
        )
</insert>

</mapper>